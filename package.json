{
  "name": "charticulator-visual-community",
  "version": "1.5.0",
  "description": "Community version of Charticulator visual",
  "repository": "https://github.com/zBritva/charticulator-visual-community",
  "author": "Ilfat Galiev",
  "license": "MIT",
  "engines": {
    "node": ">=20.1.0"
  },
  "scripts": {
    "preinstall": "git submodule update --init --recursive",
    "postinstall": "npm run install:charticulator && npm run copyfiles && npm run link:react-dom && npm run link:react && npm run link:icons",
    "link": "npm run link:react-dom && npm run link:react && npm run link:icons",
    "link:react": "npx link ./charticulator/node_modules/react",
    "link:react-dom": "npx link ./charticulator/node_modules/react-dom",
    "link:icons": "npx link ./charticulator/node_modules/@fluentui/react-icons",
    "config": "npx js-yaml config.yml > config.json",
    "install:charticulator": "cd charticulator && npm install --force",
    "build:charticulator": "npm run --prefix charticulator build",
    "start:app": "npm run --prefix charticulator start",
    "build:editor": "webpack --config editor-webpack.config",
    "start:editor": "webpack-dev-server --config editor-webpack.config",
    "build:view": "webpack --config view-webpack.config",
    "start:view": "webpack-dev-server --config view-webpack.config",
    "package": "npm run build:charticulator & npm run build:editor & npm run build:view",
    "build:visuals": "npm run build:view & npm run build:editor",
    "eslint:visual": "npx eslint ./src --ext .js,.jsx,.ts,.tsx",
    "eslint:charticulator": "npm run --prefix charticulator eslint",
    "start:charticulator": "npm run --prefix charticulator start",
    "eslint": "npm run eslint:charticulator & npm run eslint:visual",
    "audit": "npm audit",
    "copyfiles": "copyfiles .\\config.yml .\\charticulator\\"
  },
  "dependencies": {
    "@babel/core": "7.15.5",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-react": "7.18.6",
    "@fluentui/react-components": "9.54.2",
    "@fluentui/react-icons": "2.0.245",
    "@reduxjs/toolkit": "1.9.7",
    "copyfiles": "2.4.1",
    "d3-color": "3.1.0",
    "d3-dsv": "3.0.1",
    "d3-force": "3.0.0",
    "d3-format": "3.1.0",
    "d3-scale": "4.0.2",
    "d3-time-format": "4.1.0",
    "dompurify": "^2.5.7",
    "hammerjs": "2.0.8",
    "node-fetch": "2.6.7",
    "pegjs": "0.10.0",
    "pegjs-loader": "0.5.8",
    "powerbi-visuals-api": "^5.11.0",
    "powerbi-visuals-utils-dataviewutils": "2.4.1",
    "powerbi-visuals-webpack-plugin": "4.0.1",
    "raw-loader": "4.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.3",
    "sass": "1.69.5",
    "ts-node": "10.9.2"
  },
  "devDependencies": {
    "@types/d3-color": "3.1.3",
    "@types/d3-dsv": "3.0.7",
    "@types/d3-force": "3.0.9",
    "@types/d3-format": "3.0.4",
    "@types/d3-scale": "4.0.8",
    "@types/d3-time-format": "4.0.3",
    "@types/dompurify": "2.3.4",
    "@types/hammerjs": "2.0.43",
    "@types/react": "18.2.34",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "babel-loader": "9.1.3",
    "babel-plugin-module-resolver": "5.0.0",
    "base64-inline-loader": "2.0.1",
    "css-loader": "6.8.1",
    "eslint": "8.56.0",
    "eslint-plugin-powerbi-visuals": "0.8.1",
    "extra-watch-webpack-plugin": "1.0.3",
    "js-yaml": "3.13.1",
    "json-loader": "0.5.7",
    "less-loader": "11.1.3",
    "link": "2.1.0",
    "mini-css-extract-plugin": "2.3.0",
    "sass-loader": "13.3.2",
    "style-loader": "4.0.0",
    "ts-loader": "9.5.1",
    "typescript": "5.2.2",
    "webpack": "^5.94.0",
    "webpack-bundle-analyzer": "4.10.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "webpack-merge": "5.10.0"
  },
  "resolutions": {
    "@types/express-serve-static-core": "4.17.39",
    "react": "18.2.0"
  },
  "browser": {
    "[module-name]": false
  }
}
