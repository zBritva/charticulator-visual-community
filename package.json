{
  "name": "charticulator-visual-community",
  "version": "0.0.2",
  "description": "Community version of Charticulator visual",
  "repository": "https://github.com/zBritva/charticulator-visual-community",
  "author": "zBritva",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "visual",
    "charticulator"
