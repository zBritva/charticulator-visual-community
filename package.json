{
  "name": "charticulator-visual-community",
  "version": "1.0.5",
  "description": "Community version of Charticulator visual",
  "repository": "https://github.com/zBritva/charticulator-visual-community",
  "author": "Ilfat Galiev",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "charticulator-visual",
    "charticulator"
  ],
  "scripts": {
    "postinstall": "yarn copyfiles .\\config.yml .\\charticulator\\",
    "build:charticulator": "yarn workspace charticulator build",
    "build:view": "yarn workspace charticulator-visual build:view",
    "build:editor": "yarn workspace charticulator-visual build:editor",
    "start:editor": "yarn workspace charticulator-visual start:editor",
    "start:view": "yarn workspace charticulator-visual start:view",
    "build:visuals": "yarn workspace charticulator-visual build:view & yarn workspace charticulator-visual build:editor",
    "package": "yarn workspace charticulator-visual build:view & yarn workspace charticulator-visual build:editor",
    "eslint": "yarn workspace charticulator eslint & yarn workspace charticulator-visual eslint"
  },
  "dependencies": {
    "copyfiles": "2.4.1",
    "ts-node": "10.9.1"
  },
  "resolutions": {
    "@types/express-serve-static-core": "4.17.39",
    "react": "18.2.0"
  },
  "browser": {
    "[module-name]": false 
  }
}
